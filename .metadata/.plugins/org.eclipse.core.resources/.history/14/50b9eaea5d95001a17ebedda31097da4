
server:
  port: 8081

spring:
  application:
    name: 'itau'
  datasource:
    password: postgres
    username: postgres
    url: jdbc:mysql://postgresql:5432/itau
  jpa:
    hibernate:
      ddl-auto: update

eureka:
  client:
    register-with-eureka: true
    fetch-registry: true
    service-url:
      defaultZone: http://localhost:8761/eureka
